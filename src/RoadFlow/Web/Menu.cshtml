@{
    string roleID = Request.QueryString["roleid"];
    string userID = Request.QueryString["userid"];
    Guid gid,uid;
    if(!roleID.IsGuid(out gid) || !userID.IsGuid(out uid))
    {
        @:[]
    }
    else
    {
        string json = new Business.Platform.RoleApp().GetRoleAppJsonString(gid, uid);
        Response.Write(json);
    }
}