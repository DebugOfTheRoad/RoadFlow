@{
    if (!Check.CheckLogin(false) || !Check.CheckUrl())
    {
        Response.Write("验证错误!");
        Response.End();
    }

    string taskid = Request.QueryString["taskid"];
    Guid taskID;
    if(!taskid.IsGuid(out taskID))
    {
        Response.Write("参数错误!");
        Response.End();
    }
    string msg = new Business.Platform.WorkFlowTask().BackTask(taskID);

    Business.Platform.Log.Add("管理员退回了流程任务", "任务ID：" + taskid + "返回信息：" + msg, Business.Platform.Log.Types.流程相关);

    Response.Write(msg);
}