@{
    if (!Check.CheckLogin() || !Check.CheckUrl())
    {
        Response.Write("验证错误!");
        Response.End();
    }

    string connid = Request.QueryString["id"];

    Guid cid;
    if(!connid.IsGuid(out cid))
    {
        Response.Write("参数错误!");
        Response.End();
    }

    Response.Write(new Business.Platform.DBConnection().Test(cid));
}